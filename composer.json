{
    "name": "dontdrinkandroot/doctrine-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle that provides Doctrine Utilities",
    "keywords": [
        "Doctrine",
        "DBAL",
        "ORM",
        "Utils"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.2",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "doctrine/persistence": "^2.2",
        "dontdrinkandroot/common": "dev-master",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/form": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/security-core": "^5.4",
        "symfony/uid": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "graphp/graphviz": "^0.2.1",
        "liip/test-fixtures-bundle": "^1.0",
        "phpunit/phpunit": "^9.5.10",
        "symfony/framework-bundle": "^5.4",
        "symfony/phpunit-bridge": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "suggest": {
        "doctrine/data-fixtures": "For data fixture testing",
        "graphp/graphviz": "For rendering Entity Relationship diagrams"
    },
    "config": {
        "sort-packages": true
    }
}
