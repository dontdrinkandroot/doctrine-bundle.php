{
    "name": "dontdrinkandroot/doctrine-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle that provides Doctrine Utilities",
    "keywords": [
        "Doctrine",
        "DBAL",
        "ORM",
        "Utils"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/orm": "^2.17",
        "dontdrinkandroot/common": "^0.2.2",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/uid": "^6.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "graphp/graphviz": "^0.2.2",
        "liip/test-fixtures-bundle": "^2.6",
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/phpunit-bridge": "^6.4"
    },
    "suggest": {
        "doctrine/data-fixtures": "For data fixture testing",
        "graphp/graphviz": "For rendering Entity Relationship diagrams"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "require": "6.4.*",
            "allow-contrib": true
        },
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
