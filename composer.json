{
    "name": "dontdrinkandroot/doctrine-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle that provides Doctrine Utilities",
    "keywords": [
        "Doctrine",
        "DBAL",
        "ORM",
        "Utils"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/doctrine-bundle": "^1.4|^2.0",
        "dontdrinkandroot/doctrine": "0.1.x-dev",
        "dontdrinkandroot/utils": "dev-master",
        "symfony/form": "^4.0",
        "symfony/property-access": "^4.0",
        "symfony/security-core": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "doctrine/data-fixtures": "^1.3",
        "graphp/graphviz": "^0.2.1"
    },
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineBundle\\": ""
        }
    },
    "suggest": {
        "doctrine/data-fixtures": "For data fixture testing",
        "graphp/graphviz": "For rendering Entity Relationship diagrams"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
