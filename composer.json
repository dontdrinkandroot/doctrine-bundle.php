{
    "name": "dontdrinkandroot/doctrine-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle that provides Doctrine Utilities",
    "keywords": [
        "Doctrine",
        "DBAL",
        "ORM",
        "Utils"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.5",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/orm": "^3.6",
        "dontdrinkandroot/common": "^0.3",
        "symfony/config": "^7.4",
        "symfony/dependency-injection": "^7.4",
        "symfony/uid": "^7.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "dontdrinkandroot/symfony-version-enforcer": "^0.1",
        "graphp/graphviz": "^0.2.2",
        "liip/test-fixtures-bundle": "^3.7",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.5",
        "symfony/browser-kit": "^7.4",
        "symfony/framework-bundle": "^7.4",
        "symfony/runtime": "^7.4"
    },
    "suggest": {
        "doctrine/data-fixtures": "For data fixture testing",
        "graphp/graphviz": "For rendering Entity Relationship diagrams"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dontdrinkandroot/symfony-version-enforcer": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony": {
            "require": "7.4.*",
            "allow-contrib": true
        },
        "branch-alias": {
            "dev-main": "0.5.x-dev"
        }
    }
}
