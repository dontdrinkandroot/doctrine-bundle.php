{
    "name": "dontdrinkandroot/doctrine-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle that provides Doctrine Utilities",
    "keywords": [
        "Doctrine",
        "DBAL",
        "ORM",
        "Utils"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dontdrinkandroot\\DoctrineBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.5",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/orm": "^2.14",
        "doctrine/persistence": "^3.1",
        "dontdrinkandroot/common": "^0.2",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/uid": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "graphp/graphviz": "^0.2.1",
        "liip/test-fixtures-bundle": "^2.5",
        "phpunit/phpunit": "^9.5.27",
        "rector/rector": "^0.15.10",
        "symfony/flex": "^2.2",
        "symfony/framework-bundle": "^5.4",
        "symfony/phpunit-bridge": "^5.4"
    },
    "suggest": {
        "doctrine/data-fixtures": "For data fixture testing",
        "graphp/graphviz": "For rendering Entity Relationship diagrams"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "require": "5.4.*",
            "allow-contrib": true
        },
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
